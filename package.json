{
  "name": "multicodex",
  "version": "0.1.8",
  "description": "Multi-account wrapper for the Codex CLI",
  "type": "module",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build --target=node --format=esm --outfile dist/cli.js src/cli.ts && bun run scripts/postbuild.ts",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit",
    "release": "bun run scripts/release.ts"
  },
  "bin": {
    "multicodex": "dist/cli.js",
    "mcodex": "dist/cli.js"
  },
  "files": [
    "dist/cli.js",
    "README.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "docs/",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "codex",
    "cli",
    "openai",
    "multi-account"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "commander": "^14.0.3"
  }
}
